{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Attach to running n8n",
			"processId": "${command:PickProcess}",
			"request": "attach",
			"skipFiles": ["<node_internals>/**"],
			"type": "node"
		},
		{
			"name": "Launch via NPM",
			"type": "node",
			"request": "launch",
			"cwd": "${workspaceRoot}",
			"runtimeExecutable": "npm",
			"runtimeArgs": [
				"run-script", "generate-properties"
			]
		}		
	]
}
